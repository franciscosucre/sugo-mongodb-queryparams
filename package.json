  },
  "devDependencies": {
    "@types/nearley": "^2.11.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "mocha": "^5.2.0"
  },
  "keywords": [
    "queryparam",
    "queryparams",
